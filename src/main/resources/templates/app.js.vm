/*jshint globalstrict:true */
/*global angular:true */
'use strict';

/* Application level module which depends on filters, controllers, and services */
angular.module('${app}', [
    '${app}.controllers', 
    '${app}.filters', 
    '${app}.services', 
    '${app}.directives', 
    'evo'
    ]).config(['$routeProvider', function($routeProvider) {
        $routeProvider
            .when('/search', {
                templateUrl: '/test/partials/search.html'
            })
            .when('/results', {
                templateUrl: '/test/partials/results.html' 
            })
            .otherwise({
                redirectTo: '/search'
            });
  }]);